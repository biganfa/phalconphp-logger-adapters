{
  "name": "biganfa/phalconphp-logger-adapters",
  "description": "Miscellaneous Adapters For PhalconPHP standard logger",
  "keywords": [
    "phalcon",
    "phalconphp",
    "logger",
    "adapter",
    "loggy",
    "telegram"
  ],
  "license": "WTFPL",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Eleirbag89/TelegramBotPHP.git"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-phalcon": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "eleirbag89/telegrambotphp": "1.3.10",
    "ext-json": "*"
  },
  "require-dev": {
    "phalcon/ide-stubs": "*",
    "guzzlehttp/guzzle": "^7.3",
    "http-interop/http-factory-guzzle": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PhalconLogAdapters\\": "src/PhalconLogAdapters/"
    }
  }
}
